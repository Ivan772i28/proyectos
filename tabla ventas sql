CREATE TABLE VENTAS (
  FOLIO INT PRIMARY KEY,
  FECHA DATE,
  HORA TIME,
  CLAVE_EMPLEADO INT,
  CODIGO_PERECEDERO INT,
  CANTIDAD_PERECEDERO INT,
  CODIGO_ENLATADO INT,
  CANTIDAD_ENLATADO INT,
  FOREIGN KEY (CLAVE_EMPLEADO) REFERENCES EMPLEADOS(CLAVE_EMPLEADO),
  FOREIGN KEY (CODIGO_PERECEDERO) REFERENCES PERECEDEROS(CODIGO),
  FOREIGN KEY (CODIGO_ENLATADO) REFERENCES ENLATADOS(CODIGO)
);
